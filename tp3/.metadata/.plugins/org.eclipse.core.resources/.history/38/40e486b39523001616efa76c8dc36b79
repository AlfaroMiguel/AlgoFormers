package fiuba.algo3.test;

import static org.junit.Assert.*;

import org.junit.Test;

public class algoformersTest {

	@Test
	public void crearAlgoformersConMultitonTest() {
		Autobot optimus = new Autobot("optimus");
		Autobot bumblebee = new Autobot("bumblebee");
		Autobot optimus2 = new Autobot("optimus");
		Autobot bumblebee2 = new Autobot("bumblebee");
		assertEquals(optimus, optimus2);
		assertEquals(bumblebee, bumblebee2);
	}

}
